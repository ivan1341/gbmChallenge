+      r, e = new RegExp("^--?([a-z_0-9/:-]+)(?:=(.*))?", "i");
+
+  argv.forEach(function(arg){
+    var k, v;
+    if(!startsWith(arg, '-')) {
+      args.push(arg);
+    } else if(e.test(arg)) {
+      r = e.exec(arg);
+      k = r[1], v = r[2] === undefined ? true : r[2];
+
+      if(isFinite(v))
