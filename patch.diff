+var ASN_LONG_LEN  = 0x80, SSH_RSA = 'ssh-rsa';
+
+function ASN_len(s) {
+    var len = s.length;
+    if (len < ASN_LONG_LEN)
+        return new Buffer([len]);
+
+    var data = len.toString(16);
+    if(data.length & 1)
+      data = "0" + data;
+    data = new Buffer(data, 'hex');
+    return Buffer.concat([new Buffer([data.length | ASN_LONG_LEN]), data]);;
