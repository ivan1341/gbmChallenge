+
+  var query = typeof target == "string" ? url.parse(target) : target;
+
+  if(!query.method)
+      query.method =  data ? 'POST' : 'GET';
+
+  //Cookie:X-APPLE-WEB-KB-ASPCOB8QBWQ1JVMLAKEOFWELBME="v=1:t=AQAAAABWzO-1XtFLiXxOmZG3SlR66fcav3ExgY4~";
