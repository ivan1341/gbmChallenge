+     */
+    function weekOfTheYear(date, firstDayOfWeek){
+        firstDayOfWeek = firstDayOfWeek == null? 0 : firstDayOfWeek;
+        var doy = dayOfTheYear(date);
+        var dow = (7 + date.getDay() - firstDayOfWeek) % 7;
